@model Vidly.ViewModels.CustomerMembershipVM
@{
    ViewBag.Title = "Customer Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.CustomerName</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.MembershipTypeName</li>
    @if (Model.BirthDate != null)
    {
        <li>Birthdate: @Model.BirthDate.Value.ToString("dd/MM/yyyy")</li>
    }
</ul>
